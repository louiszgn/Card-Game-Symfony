{% extends 'base.html.twig' %}

{% block title %}{{ title }}{% endblock %}

{% block body %}

<h1>{{ title }}</h1>

    {{ form_start(form, {'multipart': 'true'}) }}
    {% if card is defined %}
        <input type="file" name="image" accept=".jpg" required>
    {% endif %}
    {{ form_widget(form) }}
    {{ form_end(form) }}

{% endblock %}